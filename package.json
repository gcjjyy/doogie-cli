{
  "name": "doogie-cli",
  "version": "1.0.5",
  "description": "두기의 고전게임 런처 CLI",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "doogie": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun run scripts/build.ts",
    "build:all": "bun run scripts/build.ts --all",
    "build:macos": "bun build --compile --target=bun-darwin-arm64 ./src/index.ts --outfile dist/doogie-cli",
    "build:linux": "bun build --compile --target=bun-linux-x64 ./src/index.ts --outfile dist/doogie-cli",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.30.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "cheerio": "^1.0.0",
    "cli-table3": "^0.6.5",
    "drizzle-orm": "^0.44.2",
    "p-limit": "^6.2.0",
    "picocolors": "^1.1.1"
  }
}
